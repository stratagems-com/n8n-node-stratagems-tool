{
  "name": "n8n-nodes-8kit",
  "version": "0.1.0",
  "description": "n8n nodes for 8kit Automation Tools - set tracking, lookup mapping, and app management",
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "8kit",
    "automation",
    "set-tracking",
    "lookup-mapping",
    "idempotent",
    "workflow"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Your Name",
    "email": "your@email.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/n8n-nodes-basic-processor.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "./start-n8n.sh",
    "stop": "./stop-n8n.sh",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "test:operations": "jest --testPathPattern=operations"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/EightKit/EightKit.node.js"
    ],
    "credentials": [
      "dist/credentials/EightKitApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.14",
    "@typescript-eslint/parser": "^8.37.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "gulp-svgmin": "^4.0.3",
    "jest": "^29.7.0",
    "n8n-core": "^1.14.1",
    "n8n-workflow": "^1.82.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.3",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  }
}